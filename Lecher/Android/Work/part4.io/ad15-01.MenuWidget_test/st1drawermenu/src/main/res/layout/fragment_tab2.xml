<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
             xmlns:app="http://schemas.android.com/apk/res-auto"
             xmlns:tools="http://schemas.android.com/tools"
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             tools:context=".AppBarMainActivity">
    
     <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
    
        <ImageButton
            android:id="@+id/imagebtnB1"
            android:layout_width="90dp"
            android:layout_height="80dp"
            android:background="@color/cardview_shadow_end_color"
            app:srcCompat="@drawable/coffee"/>
    
        <ImageButton
            android:id="@+id/imagebtnB2"
            android:layout_width="90dp"
            android:layout_height="80dp"
            android:background="@color/cardview_shadow_end_color"
            app:srcCompat="@drawable/google"/>
    
        <ImageButton
            android:id="@+id/imagebtnB3"
            android:layout_width="90dp"
            android:layout_height="80dp"
            android:background="@color/cardview_shadow_end_color"
            app:srcCompat="@drawable/naver"/>
    
        <ImageButton
            android:id="@+id/imagebtnB4"
            android:layout_width="90dp"
            android:layout_height="80dp"
            android:background="@color/cardview_shadow_end_color"
            app:srcCompat="@drawable/youtube"/>
    
        <ImageButton
            android:id="@+id/imagebtnB5"
            android:layout_width="90dp"
            android:layout_height="80dp"
            android:background="@color/cardview_shadow_end_color"
            app:srcCompat="@drawable/camera"/>
    
        <ImageButton
            android:id="@+id/imagebtnB6"
            android:layout_width="90dp"
            android:layout_height="80dp"
            android:background="@color/cardview_shadow_end_color"
            app:srcCompat="@drawable/hotline"/>
    
        <Button
            android:id="@+id/imagebtnB7"
            android:layout_width="90dp"
            android:layout_height="80dp"
            android:text="끝내기"/>


    </LinearLayout>


</FrameLayout>
